import RPi.GPIO as GPIO
import time

GPIO.setmode(GPIO.BCM)
relay_pins = [10, 9, 11, 25, 8]

for pin in relay_pins:
	GPIO.setup(pin, GPIO.OUT)
	GPIO.output(pin, GPIO.LOW)   #starts with relay off (if active-low)

#turn them ON one by one
for pin in relay_pins:
	GPIO.output(pin, GPIO.HIGH)
	print(f"ON")
	time.sleep(1)

#turn them OFF

for pin in relay_pins:
	GPIO.output(pin, GPIO.HIGH)  #switch of
	print(f"Relay on GPIO{pin} OFF")
GPIO.cleanup()









